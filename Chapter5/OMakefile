.PHONY: all install clean run
FILES[] =
  sample
#  5_1
#  5_2
#  5_3
#  5_4
#  5_5

USEOCAMLFIND = true
PROGRAM = main
OCAMLFLAGS += -annot -bin-annot
.DEFAULT: run

all: $(OCamlProgram $(PROGRAM), $(FILES))

clean:
     rm -fr *~ $(PROGRAM) *.opt *.cmi *.cmx *.o *.omc *.cmt *.annot

run: all
    ./$(PROGRAM)
